{
  "manifest_version": 2,

  "name": "Vernam Cipher",
  "description": "This extension generates domain specific password via Vernam Cipher",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon_32.png",
    "default_popup": "popup/index.html"
  },
  "icons": {
    "16": "icon_16.png",
    "64": "icon_64.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "activeTab"
  ]
}